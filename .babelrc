{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      },
      "presets": [["es2015", { "modules": false }]]
    }],
    "stage-2"
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName":"element-ui",
        "styleLibraryName":"theme-chalk"
      }
    ],
    "transform-vue-jsx", 
    "transform-runtime"
  ]
}
